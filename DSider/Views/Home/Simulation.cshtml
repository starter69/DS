        <a href="#" id="detailsLevelContainer" class="detailsLevel greenItem" onclick="saveComponentDetailsModel(true,1)">
        </a>
        <span id="delimiterTopLevel2" class="detailsLevel2 greenItem"> -> </span>
        <a href="#" id="Level2Container" class="detailsLevel2 greenItem">
        </a>
