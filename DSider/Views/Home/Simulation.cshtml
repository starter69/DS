@*Property Panel*@
<style>
    .RefineryLevel, .FleetLevel {
        display: none;
    }

    .electricityLB .title-box, .hydrogenLB .title-box {
        background: #4a8b78 !important;
    }

    input.form-control.propertyItem {
        padding: 0 !important;
        padding-left: 2px !important;
        color: #4a8b78 !important;
        font-size: 17px !important;
        font-weight: 600 !important;
    }

    .faq details {
        border: 1px solid #ddd;
        background: #fff;
        margin-bottom: 1.5rem;
        border-radius: 0.35rem;
        box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04);
    }

    summary {
        cursor: pointer;
        padding: 1rem;
        border-bottom: 1px solid #ddd;
    }

    #properyTable td, #properyTableOutput td {
        font-size: 13px;
        font-weight: 900;
        color: black;
    }

    .propertyItem {
        width: 70% !important;
        margin-right: 2px !important;
    }
</style>
<style>
    #tblDataSources th {
        text-align: center;
        color: black;
    }

    .context2 {
        font-size: 1.1em;
        position: absolute;
        width: 200px;
        height: auto;
        padding: 5px 0px;
        border-radius: 5px;
        top: 10;
        left: 10;
        background-color: #fff;
        box-shadow: 0 12px 15px 0 rgba(0, 0, 0, 0.24);
        color: #333;
    }

        .context2 .context_item {
            height: 32px;
            line-height: 32px;
            cursor: pointer;
            text-overflow: ellipsis;
            overflow: hidden;
            white-space: nowrap;
        }

            .context2 .context_item:hover {
                background-color: #ddd;
            }

        .context2 .inner_item {
            margin: 0px 10px;
        }

    .inner_item i {
        margin: 0 5px 0 0;
        font-weight: bold;
    }

    .context_hr {
        height: 1px;
        border-top: 1px solid #bbb;
        margin: 3px 10px;
    }
</style>
<style>
    img.iDesign {
        width: 80px !important;
        height: 80px !important;
    }

    .userChatInfo {
        font-size: 10px;
    }

    .componentComment {
        position: absolute;
        top: 0;
        padding: 10px;
        color: #4a8b78;
        cursor: pointer !important;
    }

    .outputTitle {
        width: 100%;
        text-align: center;
        border-bottom: 1px solid #838080;
        line-height: 0.1em;
        margin: 10px 0 20px;
        font-size: 16px;
    }

        .outputTitle span {
            background: #dbe2e9;
            padding: 0 10px;
            color: #768392;
        }

    .DataSourceIcons img {
        width: 60px;
        cursor: pointer;
        height: 32px;
    }

    .DataSourceIcons {
        display: inline-block;
    }

    .drawflow-delete {
        display: none !important;
    }

    .unitSpan {
        font-weight: 900;
        padding-right: 2px;
        padding-left: 2px;
        display: inline-block;
    }

    .GeneralObjectIcons img {
        width: 40px;
        cursor: pointer;
    }

    .GeneralObjectIcons {
        display: inline-block;
        padding: 3px;
    }

    .checkASMacro {
        color: green;
        position: absolute;
        top: 0;
        right: 0;
        font-size: 20px;
    }

    .backToProject {
        float: left;
        color: #4a8b78;
        cursor: pointer;
        margin-left: 20px;
        font-size: 16px;
        font-weight: 600;
    }

        .backToProject:hover {
            color: black;
        }

    .greenItem {
        color: #4a8b78;
        font-size: 17px;
    }
</style>
