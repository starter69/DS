<div class="wrapper">
    <button class="btnSimulate send-btn" id="btnSimulate">
        <span class="icon icon-anim simIcon"></span>
        Simulate
        <div class="sending-animation">
            <span class="letter">S</span>
            <span class="letter">i</span>
            <span class="letter">m</span>
            <span class="letter">u</span>
            <span class="letter">l</span>
            <span class="letter">a</span>
            <span class="letter">t</span>
            <span class="letter">e</span>
        </div>
    </button>
    <button class="btnSimulate delivered-btn" style="color:black !important;">
        <span class="check-icon"><i class="fas fa-check"></i></span>
        Simulated
    </button>
    @*<input type="button" class="btn btn-success" value="Simulate" />*@
    <div>
        <button title="Save Design" id="btnSave" style="position:absolute;right: 210px; margin-top:6px;background:#4a8b78;border:1px solid white;border-radius:5px;" type="button" class="btn btn-dark">
            <i class="far fa-save" aria-hidden="true" style="font-size:25px;"></i>
        </button>
        <button title="Clear Design" onclick="editor.clearModuleSelected()" style="position:absolute;right: 265px; margin-top:6px;background:#4a8b78;border:1px solid white;border-radius:5px;" type="button" class="btn btn-dark">
            <i class="fas fa-trash" aria-hidden="true" style="font-size:25px;"></i>
        </button>
        <a title="Export JSON File" href="/Home/downloadExportJSON?id=@downloadExportJsonURL" id="fakeHrefForDownload" class="btn btn-dark" style="position:absolute;right: 320px;color:white; margin-top:6px;background:#4a8b78;border:1px solid white;border-radius:5px;">
            <i class="fas fa-file-export" aria-hidden="true" style="font-size:25px;"></i>
        </a>
        <button data-toggle="modal" data-target="#importJSONModal" id="btnImportJSON" title="Import JSON File" style="position:absolute;right: 382px; margin-top:6px;background:#4a8b78;border:1px solid white;border-radius:5px;" type="button" class="btn btn-dark">
            <i class="fas fa-file-import" aria-hidden="true" style="font-size:25px;"></i>
        </button>
        <button onclick="doApproveSharedProject()" id="btnApproveSharedProject" title="Approve shared project" style="position:absolute;right: 445px; margin-top:6px;background:#4a8b78;border:1px solid white;border-radius:5px;" type="button" class="btn btn-dark">
            <i class="fas fa-check" aria-hidden="true" style="font-size:25px;"></i>
        </button>
    </div>
    <div style="position:absolute; left:250px; margin-top:10px;color:#4A8B78; font-weight:700;">
