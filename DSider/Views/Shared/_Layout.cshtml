                    htmlCode += '<li class="site-menu-item ' + (Val.subProjectsList.length > 0 ? 'has-sub' : '') + '">';
                    htmlCode += '<a href="' + (Val.subProjectsList.length > 0 ? 'javascript:void(0)' : '#') + '"> <span class="site-menu-title ' + mClass + '">' + Val.projectName
                        + '</span>' + (Val.subProjectsList.length > 0 ? '<span class="site-menu-arrow"></span></a>' : '</a>');
                    if (Val.subProjectsList.length > 0) {
                        htmlCode += '<ul class="site-menu-sub">';
                        jQuery(Val.subProjectsList).each(function (subIndex, subVal) {
                            var mDash = 'Dashboard';
                            if (subVal.subProject_Type == 'Carbon_Mitigation')
                                mDash = 'DashboardMitigation';
                            htmlCode += '<li class="site-menu-item">';
                            htmlCode += '<a href="/Home/' + mDash + '/' + subVal.id + '/' + subVal.name + '/' + subVal.projectID + '/' + subVal.subProject_Type + '">';
                            htmlCode += '<span class="site-menu-title" style="padding-left:3px;">' + subVal.name + '</span>';
                            htmlCode += '</a></li>';
                        });
                        htmlCode += '</ul>'
                    }
                    htmlCode += '</li>';
                });
                jQuery('#dashboardProjects').find('ul').append(htmlCode);
            },
            error: function (response) {
            },
            failure: function (response) {
            }
        });
        //Shared Projects
        $.ajax({
            url: '/api/WebAPI_Projects/getSharedProjectsForMenu',
            type: "GET",
            contentType: 'application/json',
            success: function (response) {
                var htmlCode = '';
                jQuery(response).each(function (Index, Val) {
                    var mClass = '';
                    if (Index == 0)
                        mClass = 'active';
                    htmlCode += '<li class="site-menu-item ' + mClass + '">';
                    htmlCode += '<a href="/Home/Simulation?project=' + Val.subProjectID + '&name=' + Val.subProjectName + '&folder=' + Val.projectID +
                        '&type=' + Val.subProject_Type + '">';
                    htmlCode += '<span class="site-menu-title">' + Val.subProjectName + '</span> </a> </li>'
                });
                jQuery('#sharedProjets').find('ul').html(htmlCode);
            },
            error: function (response) {
            },
            failure: function (response) {
            }
        });
        //Recent Projects
        $.ajax({
            url: '/api/WebAPI_Projects/recentSubProject',
            type: "GET",
            contentType: 'application/json',
            success: function (response) {
                var htmlCode = '';
                jQuery(response).each(function (Index, Val) {
                    var mClass = '';
                    if (Index == 0)
                        mClass = 'active';
                    htmlCode += '<li class="site-menu-item ' + mClass + '">';
                    htmlCode += '<a href="/Home/Simulation?project=' + Val.id + '&name=' + Val.name + '&folder=' + Val.projectID + '&type=' + Val.subProject_Type + '">';
                    htmlCode += '<span class="site-menu-title">' + Val.name + '</span> </a> </li>'
                });
                jQuery('#recentSubProjets').find('ul').html(htmlCode);
            },
            error: function (response) {
            },
            failure: function (response) {
            }
        });
        function logOut() {
            eraseCookie('userName');
            document.location.href = '/Account/Login';
        }
    </script>
</body>
</html>
