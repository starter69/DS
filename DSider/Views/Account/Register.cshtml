    <script>
        const Amplify = window['aws-amplify'];
        Amplify.default.configure({ Auth: {
            // REQUIRED - Amazon Cognito Identity Pool ID
            identityPoolId: 'arn:aws:cognito-idp:us-east-1:716614602740:userpool/us-east-1_yRrXXBiWo', 

            // REQUIRED - Amazon Cognito Region
            region: 'us-east-1', 

            // OPTIONAL - Amazon Cognito User Pool ID
            userPoolId: 'us-east-1_yRrXXBiWo',

            // OPTIONAL - Amazon Cognito Web Client ID
            userPoolWebClientId: '41tglo43aj89flh2b3hijqcfhu', 
            }
        });
        
        async function confirmEmail() {
            const verificationCode = jQuery('#verificationCode').val();
            const verificationEmail = jQuery('#verificationEmail').val();
            const res = await Amplify.Auth.confirmSignUp(verificationEmail, verificationCode);
            alertify.success("Your email was verified successfully.");
            jQuery('#confirmationModal').modal('hide');

            console.log('user add', verificationEmail)
            $.ajax({
                url: '/api/WebAPI_Authentication',
                type: "POST",
                contentType: 'application/json',
                data: JSON.stringify({ userName: verificationEmail }),
                success: function (response) {
                    if (response.userName != null) {
                        document.location.href = '/Account/Login';
                    }
                }
            });
        }

        jQuery(document).ready(function () {
            jQuery(document).on('click', '#btnConfirm', function() {
                jQuery('#confirmationModal').modal('show');
            });

            jQuery(document).on('click', '#btnRegister', async function () {
                if (jQuery('#inputEmail').val() == '') {
                    alertify.error('Username is mandatory.');
                    jQuery('#inputEmail').focus();
                    return;
                }
                if (jQuery('#inputPassword').val() == '') {
                    alertify.error('Password is mandatory.');
                    jQuery('#inputPassword').focus();
                    return;
                }
                const username = jQuery('#inputEmail').val();
                const password = jQuery('#inputPassword').val(); 

                try {
                    const { user } = await Amplify.Auth.signUp({
                        username,
                        password,
                        attributes: {
                            email: username
                        },
                        autoSignIn: { // optional - enables auto sign in after user is confirmed
                            enabled: true,
                        }
                    });

                    if (user.username) {
                        jQuery('#confirmationModal').modal('show');
                    } else {
                        alertify.error('Password strength is not strong enough.');
                    }
                } catch (error) {
                    alertify.error(error.message);
                }
            });
        });
    </script>
